{
  "name": "cuda-toolkit",
  "version": "0.2.8",
  "description": "GitHub Action to install the NVIDIA CUDA Toolkit",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts jest.config.js",
    "format-check": "prettier --check **/*.ts jest.config.js",
    "lint": "eslint src/**/*.ts __tests__/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jimver/cuda-toolkit.git"
  },
  "keywords": [],
  "author": "Jim Verheijde",
  "bugs": {
    "url": "https://github.com/Jimver/cuda-toolkit/issues"
  },
  "homepage": "https://github.com/Jimver/cuda-toolkit#readme",
  "volta": {
    "node": "12.22.7"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^1.1.0",
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.3.0",
    "@actions/tool-cache": "^2.0.1",
    "@actions/cache": "^3.0.6",
    "@actions/io": "^1.1.2",
    "@types/semver": "^7.3.12",
    "semver": "^7.3.8"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^18.11.2",
    "@typescript-eslint/parser": "^5.40.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-github": "^4.4.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "jest-circus": "^29.2.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.4"
  }
}
