{
  "name": "cuda-toolkit",
  "version": "0.2.7",
  "description": "GitHub Action to install the NVIDIA CUDA Toolkit",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts jest.config.js",
    "format-check": "prettier --check **/*.ts jest.config.js",
    "lint": "eslint src/**/*.ts __tests__/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jimver/cuda-toolkit.git"
  },
  "keywords": [],
  "author": "Jim Verheijde",
  "bugs": {
    "url": "https://github.com/Jimver/cuda-toolkit/issues"
  },
  "homepage": "https://github.com/Jimver/cuda-toolkit#readme",
  "volta": {
    "node": "12.22.7"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^1.0.2",
    "@actions/core": "^1.8.2",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.3.0",
    "@actions/tool-cache": "^2.0.1",
    "@types/semver": "^7.3.9",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.33",
    "@typescript-eslint/parser": "^5.23.0",
    "@vercel/ncc": "^0.33.4",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "jest-circus": "^28.1.0",
    "js-yaml": "^4.1.0",
    "prettier": "2.6.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  }
}
