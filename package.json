{
  "name": "cuda-toolkit",
  "version": "0.2.15",
  "description": "GitHub Action to install the NVIDIA CUDA Toolkit",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc --newLine lf",
    "format": "prettier --write **/*.ts jest.config.js",
    "format-check": "prettier --check **/*.ts jest.config.js",
    "lint": "eslint src/**/*.ts __tests__/**/*.ts",
    "package": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jimver/cuda-toolkit.git"
  },
  "keywords": [],
  "author": "Jim Verheijde",
  "bugs": {
    "url": "https://github.com/Jimver/cuda-toolkit/issues"
  },
  "homepage": "https://github.com/Jimver/cuda-toolkit#readme",
  "volta": {
    "node": "20.12.2"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^2.1.5",
    "@actions/cache": "^3.2.4",
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.4.0",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1",
    "@types/semver": "^7.5.8",
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
